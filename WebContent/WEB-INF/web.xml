<?xml version="1.0" encoding="ASCII"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Quizzer</display-name>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>quiz.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuizServlet</servlet-name>
    <servlet-class>quiz.QuizServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuizServlet</servlet-name>
    <url-pattern>/qz/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuizLandingServlet</servlet-name>
    <servlet-class>quiz.QuizLandingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuizLandingServlet</servlet-name>
    <url-pattern>/ql/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>quiz.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
</web-app>